;
; BIND data file for local loopback interface
;
$TTL	604800
@	  IN	SOA	ns1.digicore-19.test. usuari.digicore-19.test. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;name server NS records
	  IN	  NS	  ns1.digicore-19.test.

; registers
ns1       IN      A       192.168.4.19
server    IN      A       192.168.4.19
dbserver  IN      A       192.168.4.119
data      IN      CNAME   dbserver
